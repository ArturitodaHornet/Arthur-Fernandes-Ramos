void setup()
{
 pinMode(3, OUTPUT);
 pinMode(6, OUTPUT);
 pinMode(9, OUTPUT);
 pinMode(12,OUTPUT);
 pinMode(10,OUTPUT);

}

void loop()
{
  digitalWrite(3, HIGH);
  delay(2000);
  digitalWrite(6, HIGH);
  delay(2000); 
  digitalWrite(9, HIGH);
  delay(2000); 
  digitalWrite(12, HIGH);
  delay(2000); 
  digitalWrite(10, HIGH);
  delay(2000); 
  digitalWrite(3, LOW);
  delay(2000); 
  digitalWrite(6, LOW);
  delay(2000); 
  digitalWrite(9, LOW);
  delay(2000); 
  digitalWrite(12, LOW);
  delay(2000); 
  digitalWrite(10, LOW);
  delay(2000); 









}